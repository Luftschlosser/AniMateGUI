{
  "name": "electron-test",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "electron": "electron .",
    "electron:dev-unix": "concurrently \"NODE_ENV=development npm run dev\" \"NODE_ENV=development npm run electron\"",
    "electron:dev-win": "concurrently \"cross-env NODE_ENV=development npm run dev\" \"cross-env NODE_ENV=development npm run electron\"",
    "electron:build": "npm run build && electron-builder"
  },
  "dependencies": {
    "@emotion/styled": "^11.13.0",
    "@emotion/css": "^11.13.0",
    "@mui/icons-material": "^6.0.0",
    "@mui/material": "^6.0.0",
    "@mui/styled-engine": "^6.0.0",
    "i18next": "^23.14.0",
    "i18next-browser-languagedetector": "^8.0.0",
    "i18next-http-backend": "^2.6.1",
    "luxon": "3.4.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-i18next": "^15.0.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.0",
    "@types/luxon": "3.3.5",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/w3c-web-usb": "^1.0.10",
    "@vitejs/plugin-react": "^4.3.1",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "electron": "^32.0.1",
    "electron-builder": "^24.13.3",
    "electron-is-dev": "^3.0.1",
    "eslint": "^8.55.0",
    "globals": "^15.9.0",
    "prettier": "3.3.3",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.0.1",
    "vite": "^5.4.1"
  },
  "main": "electron/main.mjs",
  "build": {
    "appId": "com.yourapp.id",
    "files": [
      "dist/**/*",
      "electron/**/*"
    ],
    "extraMetadata": {
      "main": "electron/main.js"
    }
  }
}
